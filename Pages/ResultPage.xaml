<Page x:Class="SemestrControl.Pages.ResultPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
      Title="ResultPage">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Ваш результат:" FontSize="24" Margin="0,20,0,0"/>
            <TextBlock Text="{Binding ResultMessage}" FontSize="18" Margin="0,20,0,0"/>
        </StackPanel>
    </Grid>
</Page>
